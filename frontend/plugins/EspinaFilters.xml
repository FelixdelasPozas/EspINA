<ServerManagerConfiguration>
	<ProxyGroup name="lookup_tables">
		<LookupTableProxy name="EspinaLookupTable" class="vtkLookupTable">
		<!--This is a copy of the LUT defined inside Paraview resources/utilities
		with SetTableValue property's support-->
			<DoubleVectorProperty
				name="TableValue"
				command="SetTableValue"
				number_of_elements="5" 
				default_values="0 0 1 0 1">
				<!--DoubleRangeDomain name="range" min="0 0" max="1 1" /-->
			</DoubleVectorProperty>
			<IntVectorProperty
				name="NumberOfTableValues"
				command="SetNumberOfTableValues"
				number_of_elements="1"
				default_values="256" >
				<IntRangeDomain name="range" min="1" />
			</IntVectorProperty>

			<DoubleVectorProperty
				name="HueRange"
				command="SetHueRange"
				number_of_elements="2" 
				default_values="0 0.66667">
				<DoubleRangeDomain name="range" min="0 0" max="1 1" />
			</DoubleVectorProperty>

			<DoubleVectorProperty
				name="SaturationRange"
				command="SetSaturationRange"
				number_of_elements="2" 
				default_values="1 1">
				<DoubleRangeDomain name="range" min="0 0" max="1 1" />
			</DoubleVectorProperty>

			<DoubleVectorProperty
				name="ValueRange"
				command="SetValueRange"
				number_of_elements="2" 
				default_values="1 1">
				<DoubleRangeDomain name="range" min="0 0" max="1 1" />
			</DoubleVectorProperty>

			<DoubleVectorProperty
				name="ScalarRange"
				command="SetTableRange"
				number_of_elements="2" 
				default_values="0 1">
			</DoubleVectorProperty>

			<IntVectorProperty
				name="VectorComponent"
				command="SetVectorComponent"
				number_of_elements="1" 
				default_values="0">
				<EnumerationDomain name="enum">
					<Entry value="0" text="X" />
					<Entry value="1" text="Y" />
					<Entry value="2" text="Z" />
				</EnumerationDomain>
			</IntVectorProperty>

			<IntVectorProperty
				name="VectorMode"
				command="SetVectorMode"
				number_of_elements="1" 
				default_values="1">
				<EnumerationDomain name="enum">
					<Entry value="0" text="Magnitude" />
					<Entry value="1" text="Component" />
				</EnumerationDomain>
			</IntVectorProperty>

			<StringVectorProperty
				name="ArrayName"
				command="SetArrayName"
				number_of_elements="1"
				update_self="1"
				default_values="">
			</StringVectorProperty>

		</LookupTableProxy>
	</ProxyGroup>
        
  <ProxyGroup name="filters">
   <SourceProxy name="ImageSlicer" class="vtkPVImageSlicer" label="ImageSlicer">
     <Documentation
        long_help="Create a plane representing a slice of a given image."
        short_help="Create a slice of a given image.">
     </Documentation>
     <InputProperty
        name="Input"
        command="SetInputConnection">
           <ProxyGroupDomain name="groups">
             <Group name="sources"/>
             <Group name="filters"/>
           </ProxyGroupDomain>
           <DataTypeDomain name="input_type">
             <DataType value="vtkDataImage"/>
           </DataTypeDomain>
      </InputProperty>
     <IntVectorProperty
		 name="SliceMode"
         number_of_elements="1"
		 default_values="5"
		 immediate_update="1"
        command="SetSliceMode">
      </IntVectorProperty>
     <IntVectorProperty
		 name="Slice"
         number_of_elements="1"
         default_values="0"
		 immediate_update="1"
        command="SetSlice">
		<ProxyGroupDomain name="groups">
			<Group name="implicit_functions"/>
		</ProxyGroupDomain>
		<ProxyListDomain name="proxy_list">
			<Proxy group="implicit_functions" name="Plane" />
		</ProxyListDomain>
		<Documentation>
			Set the parameters to the implicit function used for cutting.
		</Documentation>
      </IntVectorProperty>
   </SourceProxy>
   
   <SourceProxy name="ImageChangeInformation"
                class="vtkImageChangeInformation"
                label="Change Image Information">
    <InputProperty
        name="Input"
        command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
            <Group name="filters"/>
          </ProxyGroupDomain>
          <DataTypeDomain name="input_type">
            <DataType value="vtkDataSet"/>
          </DataTypeDomain>
    </InputProperty>
    <DoubleVectorProperty
        name="OutputSpacing"
        command="SetOutputSpacing"
        immediate_update="1"
        default_values="1.0 1.0 1.0"
        number_of_elements="3"
        number_elements_per_command="3"
        repeat_command="0"
     >
    </DoubleVectorProperty>
   </SourceProxy> 

   <SourceProxy name="ImageMapToColors" class="vtkImageMapToColors" label="ImageMapToColors">
     <Documentation
		long_help="Map the input image through a lookup table"
		short_help="The vtkImageMapToColors filter will take an input 
		image of any valid scalar type, and map the first component of 
		the image through a lookup table. The result is an image of type 
		VTK_UNSIGNED_CHAR. If the lookup table is not set, or is set to NULL,
		then the input data will be passed through if it is already of 
		type VTK_UNSIGNED_CHAR.">
     </Documentation>
     <InputProperty
        name="Input"
        command="SetInputConnection">
           <ProxyGroupDomain name="groups">
             <Group name="sources"/>
             <Group name="filters"/>
           </ProxyGroupDomain>
           <DataTypeDomain name="input_type">
             <DataType value="vtkDataImage"/>
           </DataTypeDomain>
      </InputProperty>
     <ProxyProperty
        name="LookupTable"
        command="SetLookupTable">
      </ProxyProperty>
   </SourceProxy>

   <SourceProxy name="ImageBlend" class="vtkImageBlend" label="ImageBlend">
     <Documentation
        long_help="Blends images together using alph or opacity"
		short_help="Blends images together using alph or opacity">
     </Documentation>
     <InputProperty
        name="Input"
		immediate_update="1"
		port_index="0"
		clean_command="RemoveAllInputs"
		command="AddInputConnection"
		repeatable="1"
		>
           <ProxyGroupDomain name="groups">
             <Group name="sources"/>
             <Group name="filters"/>
           </ProxyGroupDomain>
           <DataTypeDomain name="input_type">
             <DataType value="vtkDataImage"/>
           </DataTypeDomain>
      </InputProperty>
   </SourceProxy>

   <SourceProxy name="ImageLabelMapBlend" class="vtkImageLabelMapBlend" label="ImageLabelMapBlend">
     <Documentation
        long_help="Blends images together using alpha or opacity"
		short_help="Blends images together using alpha or opacity">
     </Documentation>
     <InputProperty
        name="Input"
		port_index="0"
		immediate_update="1"
		clean_command="RemoveAllInputs"
		command="AddInputConnection"
		repeatable="1"
		>
           <ProxyGroupDomain name="groups">
             <Group name="sources"/>
             <Group name="filters"/>
           </ProxyGroupDomain>
           <DataTypeDomain name="input_type">
             <DataType value="vtkDataImage"/>
           </DataTypeDomain>
      </InputProperty>
      <DoubleVectorProperty
	name="InputColor"
	number_of_elements="4"
	immediate_update="1"
	repeatable="1"
	command="SetLabelMapColor">
      </DoubleVectorProperty>
      </SourceProxy>
      
     <SourceProxy name="MorphologicalFeatures" class="vtkMorphologicalFeaturesFilter" label="MorphologicalFeatures">
     <Documentation
       short_help="Extract morphological features from its input"
       long_help="Extract morphological features from its input">
     </Documentation>
     <InputProperty
        name="Input"
		port_index="0"
		immediate_update="1"
		command="SetInputConnection"
		>
           <ProxyGroupDomain name="groups">
             <Group name="sources"/>
             <Group name="filters"/>
           </ProxyGroupDomain>
           <DataTypeDomain name="input_type">
             <DataType value="vtkDataImage"/>
           </DataTypeDomain>
      </InputProperty>
      <DoubleVectorProperty
	name="Size"
	number_of_elements="1"
	immediate_update="1"
	information_only="1"
	command="GetSize">
	<SimpleDoubleInformationHelper/>
      </DoubleVectorProperty> 
      <DoubleVectorProperty
	name="PhysicalSize"
	number_of_elements="1"
	immediate_update="1"
	information_only="1"
	command="GetPhysicalSize">
	<SimpleDoubleInformationHelper/>
      </DoubleVectorProperty>
      <DoubleVectorProperty
	name="Centroid"
	number_of_elements="3"
	immediate_update="1"
	information_only="1"
	command="GetCentroid">
	<SimpleDoubleInformationHelper/>
      </DoubleVectorProperty>
      <IntVectorProperty
	name="Region"
	number_of_elements="3"
	immediate_update="1"
	information_only="1"
	command="GetRegion">
	<SimpleIntInformationHelper/>
      </IntVectorProperty>
      <DoubleVectorProperty
	name="BinaryPrincipalMoments"
	number_of_elements="3"
	immediate_update="1"
	information_only="1"
	command="GetBinaryPrincipalMoments">
	<SimpleDoubleInformationHelper/>
      </DoubleVectorProperty>
      <DoubleVectorProperty
	name="BinaryPrincipalAxes"
	number_of_elements="9"
	immediate_update="1"
	information_only="1"
	command="GetBinaryPrincipalAxes">
	<SimpleDoubleInformationHelper/>
      </DoubleVectorProperty>
      <DoubleVectorProperty
	name="FeretDiameter"
	number_of_elements="1"
	immediate_update="1"
	information_only="1"
	command="GetFeretDiameter">
	<SimpleDoubleInformationHelper/>
      </DoubleVectorProperty>
      <DoubleVectorProperty
	name="EquivalentEllipsoidSize"
	number_of_elements="3"
	immediate_update="1"
	information_only="1"
	command="GetEquivalentEllipsoidSize">
	<SimpleDoubleInformationHelper/>
      </DoubleVectorProperty>
      </SourceProxy>

   <SourceProxy name="MyWindowLevelLookupTable" class="vtkWindowLevelLookupTable" label="WindowLevelLookupTable">
     <Documentation
        long_help="Map scalar values into colors or colors to scalars; generate color table"
		short_help="">
     </Documentation>
     <DoubleVectorProperty
		 name="Window"
         number_of_elements="1"
		 default_values="255"
		 immediate_update="1"
        command="SetWindow">
      </DoubleVectorProperty>
     <DoubleVectorProperty
		 name="Level"
         number_of_elements="1"
		 default_values="127.5"
		 immediate_update="1"
        command="SetLevel">
      </DoubleVectorProperty>
     <IntVectorProperty
		 name="Ramp"
         number_of_elements="1"
		 default_values="0"
		 immediate_update="1"
        command="SetRamp">
      </IntVectorProperty>
   </SourceProxy>
 </ProxyGroup>
</ServerManagerConfiguration>

