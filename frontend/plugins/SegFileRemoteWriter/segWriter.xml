<ServerManagerConfiguration>
  <ProxyGroup name="filters">

    <SourceProxy name="segFileWriter"
                 class="vtkSegWriter"
                 label="MySegFileWriter">
      <Documentation short_help="Write a seg file.">
        
      </Documentation>


      <StringVectorProperty
            name="FileName"
            animateable="0"
            command="SetFileName"
            number_of_elements="1">
        <FileListDomain name="files"/>
      </StringVectorProperty>

      <StringVectorProperty
        name="Taxonomy"
        command="SetTaxonomy"
        number_of_elements="1"
        >
        <Documentation>
          This property set the content of the Taxonomy
        </Documentation>
      </StringVectorProperty>
      
      <StringVectorProperty
        name="Trace"
        command="SetTrace"
        number_of_elements="1"
        >
        <Documentation>
          This property set the content of the Trace
        </Documentation>
        <SimpleStringInformationHelper/>
      </StringVectorProperty>

    </SourceProxy>

  </ProxyGroup>

</ServerManagerConfiguration>
